  {
    "name": "Type Here",
    "description" : "Practice typing on any website",
    "version": "0.1",
    "manifest_version": 2,
    "permissions": ["contextMenus"],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["content.js"]
        }
    ],
    "web_accessible_resources": [
        "bar.html"
    ]
  }